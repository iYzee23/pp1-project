program CodeGen1
{
	void main()
	int arr1[];
	bool arr2[];
	char arr3[];
	{
		arr1 = new int[4];
		arr1[0] = 0;
		arr1[1] = 1;
		arr1[2] = 2;
		arr1[3] = 3;
		print(arr1); print(eol);
		print(arr1, 1); print(eol);
		print(eol);
		
		arr2 = new bool[7];
		arr2[0] = true;
		arr2[1] = false;
		arr2[2] = true;
		arr2[3] = false;
		arr2[4] = true;
		arr2[5] = false;
		arr2[6] = true;
		print(arr2); print(eol);
		print(arr2, 1); print(eol);
		print(eol);
		
		arr3 = new char[11];
		arr3[0] = 'P';
		arr3[1] = 'E';
		arr3[2] = 'D';
		arr3[3] = 'J';
		arr3[4] = 'A';
		arr3[5] = ' ';
		arr3[6] = 'P';
		arr3[7] = 'E';
		arr3[8] = 'S';
		arr3[9] = 'I';
		arr3[10] = 'C';
		print(arr3); print(eol);
		print(arr3, 1); print(eol);
		print(eol);
	}
}