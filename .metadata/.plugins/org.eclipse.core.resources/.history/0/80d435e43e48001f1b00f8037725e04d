public class Test303 {

    public static void main(String[] args) {
        Predmeti.Predmet[] predmeti;
        Predmeti.Sfera s1, s2, s3;
        Predmeti.Kvadar k1, k2, k3;
        int t;

        if (ProstiBrojevi.prosti[9] == 29)
            System.out.print(1);
        else
            System.out.print(0);
        System.out.println();

        predmeti = new Predmeti.Predmet[6];
        s1 = new Predmeti.Sfera();
        s1.postaviPoluprecnik(1);
        s1.postaviVrstu('s');
        s2 = new Predmeti.Sfera();
        s2.postaviPoluprecnik(2);
        s2.postaviVrstu('s');
        s3 = new Predmeti.Sfera();
        s3.postaviPoluprecnik(3);
        s3.postaviVrstu('s');

        k3 = new Predmeti.Kvadar();
        k3.postaviStranice(1, 2, 3);
        k3.postaviVrstu('k');
        k1 = new Predmeti.Kvadar();
        k1.postaviStranice(2, 3, 4);
        k1.postaviVrstu('k');
        k2 = new Predmeti.Kvadar();
        k2.postaviStranice(3, 4, 5);
        k2.postaviVrstu('k');

        predmeti[0] = s1;
        predmeti[2] = s2;
        predmeti[4] = s3;
        predmeti[1] = k1;
        predmeti[3] = k2;
        predmeti[5] = k3;

        t = 3; // You can change this to read from input

        for (int i = 0; i < 6; i++) {
            predmeti[i].postaviTezinu(t);
        }

        int zapreminaS = 0;
        int tezinaS = 0;
        for (int i = 1; i <= 6;) {
            if (i % 2 == 0) {
                i++;
                continue;
            }
            zapreminaS += predmeti[i - 1].zapremina();
            tezinaS += predmeti[i - 1].Q();
            ispis(predmeti[i - 1]);
            i++;
        }

        System.out.println();

        int zapreminaK = 0;
        int tezinaK = 0;
        for (int i = 1; i <= 6;) {
            if (i % 2 == 1) {
                i++;
                continue;
            }
            zapreminaK += predmeti[i - 1].zapremina();
            tezinaK += predmeti[i - 1].Q();
            ispis(predmeti[i - 1]);
            i++;
        }

        System.out.println();
        System.out.println(zapreminaS);
        System.out.println(zapreminaK);
        System.out.println(tezinaS);
        System.out.println(tezinaK);
    }

    public static void ispis(Predmeti.Predmet p) {
        p.ispisi(); // prints kind (char) and weight * volume
        System.out.println(p.zapremina()); // print volume (needed for sum)
        System.out.println(p.dohvTezinu()); // prints weight (set)
        System.out.println();
    }

    static class Predmeti {
        static final int cinilac = 4;
        static final int delilac = 3;

        static class Predmet {
            private int tezina;
            private char vrsta;

            public void postaviTezinu(int tezina) {
                this.tezina = tezina;
            }

            public void ucitajTezinu() {
                // read(tezina); // Implementation needed for reading input
            }

            public int zapremina() {
                return 0;
            }

            public int Q() {
                return zapremina() * tezina;
            }

            public void postaviVrstu(char vrsta) {
                this.vrsta = vrsta;
            }

            public char dohvVrstu() {
                return vrsta;
            }

            public int dohvTezinu() {
                return tezina;
            }

            public void ispisi() {
                System.out.print(vrsta);
                System.out.print(Q());
            }
        }

        static class Sfera extends Predmet {
            private int r;

            public void postaviPoluprecnik(int r) {
                this.r = r;
            }

            @Override
            public int zapremina() {
                int z = cinilac;
                return z * r / delilac;
            }
        }

        static class Kvadar extends Predmet {
            private int a, b, c;

            public void postaviStranice(int a, int b, int c) {
                this.a = a;
                this.b = b;
                this.c = c;
            }

            @Override
            public int zapremina() {
                return a * b * c;
            }
        }
    }

    static class ProstiBrojevi {
        static int[] prosti = new int[10];
        static {
            System.out.print('s'); System.out.print('t'); System.out.print('a'); System.out.print('t'); System.out.print('i'); System.out.print('c'); System.out.println();
            int X = 0;
            for (int i = 2; X < 10; i++) {
                boolean flag = true;
                for (int j = 2; j < i; j++) {
                    if (i % j == 0) {
                        flag = false;
                        break;
                    }
                }
                if (flag) {
                    prosti[X] = i;
                    X++;
                }
            }
        }
    }
}
