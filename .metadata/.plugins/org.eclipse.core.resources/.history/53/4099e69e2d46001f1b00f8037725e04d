            print(theirField.myField);
            print(eol);
            
            this.myField = 5;
            myField = 2;
            this.theirField.yourField = 'a';
            i = 5;
            this.i = 10;
            print(this.myField);
            print(myField);
            print(this.theirField.yourField);
            print(i);
            print(this.i);
            print(eol);
            
            j = new MyClass();
