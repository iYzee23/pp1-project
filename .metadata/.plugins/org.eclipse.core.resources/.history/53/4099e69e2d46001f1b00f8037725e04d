    int i;

    {
        MyClass myMethod(int param) 
        int i;
        MyClass j;
        {   
            for (i = 0; i < param; i++) {
                if (i % 2 == 0) {
                    print(i, 6);
                } else {
                    // read(i);
                    print(i + 200, 4);
                }
            }
            print(eol);
            
            ourField = true;
            for (i = 0; ourField; i++) {
            	if (i < param/2) print(i);
            	else if (i < param) print(-10 * i);
            	else ourField = false; 
            }
            print(eol);
        
            MyClass.g.myField = 2;
            print(MyClass.g.myField);
            print(eol);
            
