const int MAX_VALUE = 100;
const int MAX_INT = 5;
const char MAX_CHAR = 'A';
const bool MAX_BOOL = true;

int globArr[];
int globVar;

class MyClass 
{
    static int a, b, c[];
    static bool d, e[];
    static MyClass g, f[];
    static int pr;
    
    static 
    {
        a = 5;
        b = -5;
        print(a);
        print(b);
        print(eol);
        
        c = new int[2];
        c[1] = 6;
        d = false;
        print(c[1]);
        print(d);
        print(eol);
        
        e = new bool[3];
        e[2] = true;
