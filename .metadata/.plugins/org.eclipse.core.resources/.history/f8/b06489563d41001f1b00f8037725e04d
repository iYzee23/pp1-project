program CodeGen1
{
	void main()
	{
		print(1);
		print(1*6/2);
		print(6/2*3%5);
		print(6*0);
		// print(6/0);
		// print(6%0);
		print('a');
		print(true);
		print(false);
		
		print(1, 2);
		print(1*6/2, 2);
		print(6/2*3%5, 2);
		print(6*0, 2);
		// print(6/0, 2);
		// print(6%0, 2);
		print('a', 2);
		print(true, 2);
		print(false, 2);
	}
}